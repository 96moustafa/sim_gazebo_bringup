{	"setup":{
		"autopilot": {
			"autopilot_build_params_0": {
				"repo": "git@github.com:rudislabs/PX4-Autopilot.git",
				"name": "TII-Autopilot",
				"version": "tii_v2",
				"clone": 1,
				"source_gazebo_plugins": 0,
				"build_type": "px4_sitl_default",
				"build_prefix": "DONT_RUN=1",
				"build_postfix": ""
			}
		},
		"gazebo": {
			"gazebo_models": {
				"gazebo_repo_0":{
					"repo": "git@github.com:rudislabs/tii_gazebo.git",
					"version": "fire_turtle",
					"name": "tii_gazebo"
				},
				"gazebo_model_repo_1":{
					"repo": "git@github.com:osrf/gazebo_models.git",
					"version": "master",
					"name": "osrf/models"
				}
			},
			"gazebo_plugins": {
				"gazebo_plugin_repo_0":{
					"repo": "git@github.com:rudislabs/PX4-SITL_gazebo.git",
					"version": "gps_attack",
					"name": "tii_gazebo_plugins",
					"workspace_relative_mavlink": "TII-Autopilot/mavlink/include/",
					"source_gazebo_plugins": 1
				}
			}
		},
		"ros2": {
		},
		"system": {
			"connections": {
				"connection_0":{
					"force_unbind": true,
					"transport": "udp",
					"ports": [2019, 2020, 2021, 2022, 2023, 2024, 2025, 2026]
				},
				"connection_1":{
					"force_unbind": false,
					"transport": "tcp",
					"ports": []
				}
			},
			"set_environment": {
				"environment_0": {
					"variable": "GAZEBO_MODEL_DATABASE_URI",
					"value": "",
					"method": "overwrite"
				},
				"environment_1": {
					"variable": "GAZEBO_RESOURCE_PATH",
					"value": "/usr/share/gazebo-11",
					"method": "prepend"
				},
				"environment_2": {
					"variable": "JAVA_HOME",
					"value": "/usr/lib/jvm/java-8-openjdk-amd64/",
					"method": "ignore"
				}
			}
		}
	},
	"verbose": {
		"gzclient": true,
		"gzserver": true
	},
	"nodes": {
		"ros2_node_0":{
			"timing": "post-spawn",
			"package": "rqt_camera_gazebo",
			"executable": "rqt_camera_gazebo",
			"name": "rqt_camera_gazebo_0",
			"output": "screen",
			"parameters": [	{"camera_sub": "rgb_cam/image_raw"},
							{"camera_pub": "rqt_rgb_cam/image_raw"},
							{"camera_format": "bgr8"}
						]
		},
		"ros2_node_1":{
			"timing": "post-spawn",
			"package": "rqt_camera_gazebo",
			"executable": "rqt_camera_gazebo",
			"name": "rqt_camera_gazebo_1",
			"output": "screen",
			"parameters": [	{"camera_sub": "realsense_d455_0_camera/image_raw"},
							{"camera_pub": "rqt_d455/image_raw"},
							{"camera_format": "bgr8"}
						]
		},
		"ros2_node_2":{
			"timing": "post-spawn",
			"package": "rqt_camera_gazebo",
			"executable": "rqt_camera_gazebo",
			"name": "rqt_camera_gazebo_2",
			"output": "screen",
			"parameters": [	{"camera_sub": "realsense_d455_0_depth_camera/depth/image_raw"},
							{"camera_pub": "rqt_d455_depth/depth"},
							{"camera_format": "32FC1"}
						]
		},
		"ros2_node_4":{
			"timing": "post-spawn",
			"package": "rqt_image_view",
			"executable": "rqt_image_view",
			"name": "rqt_image_view_0",
			"output": "screen",
			"parameters": []
		}
	},
